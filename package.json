{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "devs": "sirv public --no-clear -H localhost -k -C ~/.config/ssl/localhost.crt -K ~/.config/ssl/localhost.key",
    "up": "surge public",
    "deploy": "surge public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "surge": "^0.23.1",
    "svelte-forms-lib": "^2.0.1",
    "svelte-select": "^4.4.7",
    "svelte-tags-input": "github:addiebarron/svelte-tags-input",
    "yup": "^0.32.11"
  }
}
